#include <boost/dll/import.hpp>
#include <boost/di.hpp>
#include "ioperation.hpp"
#include <iostream>

namespace di = boost::di;
namespace dll = boost::dll;

class app {
 private:
 	ioperation sum_ptr;
 public:
  app(ioperation& sum)
	 : sum_ptr(sum)
  { }

  int result() {
	  return sum_ptr->calculate(1,1);
  }
};


int main() {

	const auto injector = di::make_injector(
	  di::bind<ioperation>.to<dot_product>();
	);
	injector.create<app>();
}


